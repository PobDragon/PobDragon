<!--
    The string named "app_name" defines the application's visible name.
    The string array "Planets" defines an array of 9 strings. The application loads
    this array into the spinner's array adapter.
    The string "planet_prompt" defines the prompt for the result text box.
 -->
<resources>
    <string name="app_name">Spinner</string>
    <string-array name="Manouvres">
        <item>Attack</item>
        <item>Defence</item>
        <item>Other</item>
    </string-array>
    <string name="manouvre_prompt">Select a Manouvre</string>
    <!-- spinner2  -->
    <string-array name="Attack">
        <item>Att - Attack</item>
        <item>Att - Charge</item>
        <item>Def - Stand fast</item>
        <item>Def - Stand with another</item>
    </string-array>
    <string name="attack_prompt">subclass Manouvres</string>
</resources>